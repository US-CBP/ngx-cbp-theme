{
  "name": "ngx-cpb-theme",
  "version": "0.2.0",
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "gulp clean && gulp build",
    "build:watch:lib": "gulp clean && gulp",
    "docs": "npm run docs:build:lib",
    "docs:build:lib": "compodoc -p ./src/tsconfig.app.json -n ngx-cpb-theme -d docs --hideGenerator",
    "docs:serve:lib": "npm run docs:build:lib -- -s",
    "docs:watch:lib": "npm run docs:build:lib -- -s -w",
    "lint": "ng lint",
    "start:demo": "ng serve --app demo --port 4100",
    "test": "ng test",
    "postinstall": "gulp clean && node ./postinstall.js && npm run build",
    "publish": "npm run build:lib && node tools/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/US-CBP/ngx-cbp-theme.git"
  },
  "keywords": [
    "angular"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/US-CBP/ngx-cbp-theme.git/issues"
  },
  "dependencies": {
    "@angular/animations": "^4.3.3",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "cbp-theme": "^1.3.1"
  },
  "devDependencies": {
    "@angular/cli": "1.2.6",
    "@angular/compiler-cli": "^4.3.3",
    "@angular/language-service": "^4.3.3",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "es6-shim": "^0.35.3",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
