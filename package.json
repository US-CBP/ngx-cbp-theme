{
  "name": "ngx-cbp-theme",
  "version": "2.2.6",
  "main": "./bundles/ngx-cbp-theme.umd.js",
  "module": "./ngx-cbp-theme.es5.js",
  "es2015": "./ngx-cbp-theme.js",
  "typings": "./ngx-cbp-theme.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/US-CBP/ngx-cbp-theme.git"
  },
  "keywords": [
    "angular"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/US-CBP/ngx-cbp-theme.git/issues"
  },
  "scripts": {
    "ng": "ng",
    "build:lib": "ng build ngx-cbp-theme",
    "docs": "npm run docs:build:lib",
    "docs:build:lib": "compodoc -p ./src/tsconfig.app.json -n ngx-cpb-theme -d docs --hideGenerator",
    "docs:serve:lib": "npm run docs:build:lib -- -s",
    "docs:watch:lib": "npm run docs:build:lib -- -s -w",
    "lint": "ng lint",
    "start:kitchensink": "ng serve --app kitchensink --port 4100",
    "build:kitchensink:prod": "rimraf ./dist-kitchensink && ng build --prod --aot --build-optimizer --vendor-chunk true --app kitchensink",
    "test:kitchensink": "ng test --app kitchensink",
    "test:kitchensink:once": "ng test --app kitchensink --watch=false",
    "test:lib": "ng test --app lib",
    "test:lib:once": "ng test --app lib --watch=false",
    "e2e:kitchensink": "ng e2e --app kitchensink --watch=false",
    "ready": "npm-run-all --parallel build:kitchensink:prod build:lib lint test:once e2e:kitchensink",
    "test:once": "npm-run-all --serial  test:lib:once test:kitchensink:once",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "cbp-theme": "1.9.0",
    "classlist.js": "1.1.20150312",
    "del": "3.0.0",
    "hammerjs": "2.0.8",
    "replace": "1.0.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "web-animations-js": "2.3.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular-devkit/core": "0.6.3",
    "@angular/animations": "6.0.2",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.1.2",
    "codelyzer": "4.3.0",
    "core-js": "2.5.6",
    "es6-shim": "0.35.3",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "ng-packagr": "^3.0.0-rc.2",
    "npm-run-all": "^4.1.3",
    "protractor": "5.3.2",
    "rxjs": "6.1.0",
    "standard-version": "4.3.0",
    "tota11y": "0.1.6",
    "ts-node": "6.0.3",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "zone.js": "0.8.26"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
